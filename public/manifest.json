{
  "manifest_version": 2,
  "name": "Tabio",
  "description": "An open-source tab switcher for Chrome",
  "version": "2.0.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"]
}
