{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "max-len": "off",
    "no-shadow": "off",
    "react/jsx-wrap-multilines": [
      "error",
      { "declaration": false, "assignment": false, "arrow": false }
    ]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "chrome": false
  }
}
